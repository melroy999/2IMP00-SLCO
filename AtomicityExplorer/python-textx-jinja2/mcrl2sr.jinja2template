{% endfor %}
{% endfor %}

map up_d': Object_SM' # Var' # Bool # AccessPattern' # D_map' -> D_map';
var o': Object_SM';
    v': Var';
    a': AccessPattern';
    dm': D_map';
{% for o in model.objects | select("o_hassharedaccesses") %}
{% set oloop = loop %}
{% for sm in o.type.statemachines | select("sm_hassharedaccesses", o) %}
{% set smloop = loop %}
{% for v in accessed_sharedvars[o.type][sm][0] %}
{% if oloop.first and smloop.first and loop.first %}eqn {% endif %}(o' == O'SM'({{o.name}},{{sm.name}}) && v' == {{mcrl2varprefix}}{{v}}) -> up_d'(o', v', false, a', dm') = DMAP'({% for o2 in model.objects | select("o_hassharedaccesses") %}{% set o2loop = loop %}{% for sm2 in o.type.statemachines | select("sm_hassharedaccesses", o) %}{% set sm2loop = loop %}{% for v2 in accessed_sharedvars[o2.type][sm2][0] %}{{', ' if not (o2loop.first and sm2loop.first and loop.first) }}{% if o == o2 and sm == sm2 and v == v2 %}union'(r'{{sm2.name}}'{{v2|replace("(","_")|replace(")","")}}(dm'),a'){% else %}r'{{sm2.name}}'{{v2|replace("(","_")|replace(")","")}}(dm'){% endif %}{% endfor %}{% for v2 in accessed_sharedvars[o2.type][sm2][1] %}{{', ' if not (o2loop.first and sm2loop.first and loop.first and accessed_sharedvars[o2.type][sm2][0] == []) }}w'{{sm2.name}}'{{v2|replace("(","_")|replace(")","")}}(dm'){% endfor %}{% endfor %}{% endfor %});
{% endfor %}
{% for v in accessed_sharedvars[o.type][sm][1] %}
{% if oloop.first and smloop.first and loop.first and accessed_sharedvars[o.type][sm][0] == [] %}eqn {% endif %}(o' == O'SM'({{o.name}},{{sm.name}}) && v' == {{mcrl2varprefix}}{{v}}) -> up_d'(o', v', true, a', dm') = DMAP'({% for o2 in model.objects | select("o_hassharedaccesses") %}{% set o2loop = loop %}{% for sm2 in o.type.statemachines | select("sm_hassharedaccesses", o) %}{% set sm2loop = loop %}{% for v2 in accessed_sharedvars[o2.type][sm2][0] %}{{', ' if not (o2loop.first and sm2loop.first and loop.first) }}r'{{sm2.name}}'{{v2|replace("(","_")|replace(")","")}}(dm'){% endfor %}{% for v2 in accessed_sharedvars[o2.type][sm2][1] %}{{', ' if not (o2loop.first and sm2loop.first and loop.first and accessed_sharedvars[o2.type][sm2][1] == []) }}{% if o == o2 and sm == sm2 and v == v2 %}union'(w'{{sm2.name}}'{{v2|replace("(","_")|replace(")","")}}(dm'),a'){% else %}w'{{sm2.name}}'{{v2|replace("(","_")|replace(")","")}}(dm'){% endif %}{% endfor %}{% endfor %}{% endfor %});
{% endfor %}
{% endfor %}
{% endfor %}

