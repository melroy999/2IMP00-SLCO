{% if oloop.first and smloop.first and loop.first %}eqn {% endif %}(o' == O'SM'({{o.name}},{{sm.name}}) && v' == {{mcrl2varprefix}}{{v}}) -> get_d'(o', v', false, dm') = r'{{sm.name}}'{{v|replace("(","_")|replace(")","")}}(dm');
{% endfor %}
{% for v in accessed_sharedvars[o.type][sm][1] %}
{% if oloop.first and smloop.first and loop.first and accessed_sharedvars[o.type][sm][0] == [] %}eqn {% endif %}(o' == O'SM'({{o.name}},{{sm.name}}) && v' == {{mcrl2varprefix}}{{v}}) -> get_d'(o', v', true, dm') = w'{{sm.name}}'{{v|replace("(","_")|replace(")","")}}(dm');
