model TestDet {
    classes
    P {
        variables Integer i
        state machines
        SM0 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    not (i = 3);
                }
                from SMC0 to SMC1 {
                    i = 3;
                }
        }
        SM1 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    i <> 3;
                }
                from SMC0 to SMC1 {
                    i >= 5;
                }
                from SMC0 to SMC1 {
                    i < 1;
                }
                from SMC0 to SMC1 {
                    i = 2;
                }
        }
        SM2 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    i = 0;
                }
                from SMC0 to SMC1 {
                    i = 1;
                }
                from SMC0 to SMC1 {
                    i = 2;
                }
                from SMC0 to SMC1 {
                    i = 3;
                }
        }
        SM3 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    i = 0;
                }
                from SMC0 to SMC1 {
                    i < 0;
                }
                from SMC0 to SMC1 {
                    i = 1 + 2;
                }
                from SMC0 to SMC1 {
                    i = 2 + 1;
                }
                from SMC0 to SMC1 {
                    i = 3;
                }
        }
        SM4 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    i >= 5;
                }
                from SMC0 to SMC1 {
                    i = 5;
                }
                from SMC0 to SMC1 {
                    i = 3;
                }
                from SMC0 to SMC1 {
                    i <= 2;
                }
                from SMC0 to SMC1 {
                    i = 2;
                }
        }
        SM5 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    i < 5;
                }
                from SMC0 to SMC1 {
                    i >= 5;
                }
        }
        SM6 {
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    [i < 5; i := 0];
                }
                from SMC0 to SMC1 {
                    [i >= 5; i := 0];
                }
        }
        SM7 {
            variables Integer j
            initial SMC0 states SMC1
            transitions
                from SMC0 to SMC1 {
                    j < 5;
                }
                from SMC0 to SMC1 {
                    j >= 5;
                }
        }
    }
    objects p: P(i:=0)
}
